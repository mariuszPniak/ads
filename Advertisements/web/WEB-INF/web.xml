<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>RegisterServlet</servlet-name>
        <servlet-class>Servlets.RegisterServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>Servlets.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>Servlets.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdvertServlet</servlet-name>
        <servlet-class>Servlets.AdvertServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>IndexServlet</servlet-name>
        <servlet-class>Servlets.IndexServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PhotoServlet</servlet-name>
        <servlet-class>Servlets.PhotoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>searchServlet</servlet-name>
        <servlet-class>Servlets.searchServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>adServlet</servlet-name>
        <servlet-class>Servlets.adServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UServlet</servlet-name>
        <servlet-class>Servlets.UServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserServlet</servlet-name>
        <servlet-class>Servlets.UserServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ChangeAccountServlet</servlet-name>
        <servlet-class>Servlets.ChangeAccountServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteServlet</servlet-name>
        <servlet-class>Servlets.DeleteServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SendServlet</servlet-name>
        <servlet-class>Servlets.SendServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MessageServlet</servlet-name>
        <servlet-class>Servlets.MessageServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ResponseServlet</servlet-name>
        <servlet-class>Servlets.ResponseServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegisterServlet</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdvertServlet</servlet-name>
        <url-pattern>/advert</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>IndexServlet</servlet-name>
        <url-pattern>/index.html</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PhotoServlet</servlet-name>
        <url-pattern>/photo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserServlet</servlet-name>
        <url-pattern>/user</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ChangeAccountServlet</servlet-name>
        <url-pattern>/change-account</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>searchServlet</servlet-name>
        <url-pattern>/search</url-pattern>
        <url-pattern>/zatrudnie</url-pattern>
        <url-pattern>/poszukuje_pracy</url-pattern>
        <url-pattern>/oferuje</url-pattern>
        <url-pattern>/poszukuje_uslug</url-pattern>
        <url-pattern>/finansowe</url-pattern>
        <url-pattern>/budowlane</url-pattern>
        <url-pattern>/kupie</url-pattern>
        <url-pattern>/sprzedam</url-pattern>
        <url-pattern>/gry</url-pattern>
        <url-pattern>/akcesoria</url-pattern>
        <url-pattern>/wymienie</url-pattern>
        <url-pattern>/inne</url-pattern>
        <url-pattern>/audi</url-pattern>
        <url-pattern>/bmw</url-pattern>
        <url-pattern>/ford</url-pattern>
        <url-pattern>/renault</url-pattern>
        <url-pattern>/toyota</url-pattern>
        <url-pattern>/motocykle</url-pattern>
        <url-pattern>/czesci</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>adServlet</servlet-name>
        <url-pattern>/ogloszenie</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UServlet</servlet-name>
        <url-pattern>/users</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteServlet</servlet-name>
        <url-pattern>/delete</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SendServlet</servlet-name>
        <url-pattern>/send</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MessageServlet</servlet-name>
        <url-pattern>/message</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ResponseServlet</servlet-name>
        <url-pattern>/response</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
