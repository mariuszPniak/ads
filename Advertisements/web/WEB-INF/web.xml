<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>RegisterServlet</servlet-name>
        <servlet-class>DB.RegisterServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>DB.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>DB.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdvertServlet</servlet-name>
        <servlet-class>DB.AdvertServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>IndexServlet</servlet-name>
        <servlet-class>DB.IndexServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PhotoServlet</servlet-name>
        <servlet-class>DB.PhotoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>searchServlet</servlet-name>
        <servlet-class>DB.searchServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>adServlet</servlet-name>
        <servlet-class>DB.adServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>userServlet</servlet-name>
        <servlet-class>DB.userServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegisterServlet</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdvertServlet</servlet-name>
        <url-pattern>/advert</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>IndexServlet</servlet-name>
        <url-pattern>/index.html</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PhotoServlet</servlet-name>
        <url-pattern>/photo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>searchServlet</servlet-name>
        <url-pattern>/search</url-pattern>
        <url-pattern>/zatrudnie</url-pattern>
        <url-pattern>/poszukuje_pracy</url-pattern>
        <url-pattern>/oferuje</url-pattern>
        <url-pattern>/poszukuje_uslug</url-pattern>
        <url-pattern>/finansowe</url-pattern>
        <url-pattern>/budowlane</url-pattern>
        <url-pattern>/kupie</url-pattern>
        <url-pattern>/sprzedam</url-pattern>
        <url-pattern>/gry</url-pattern>
        <url-pattern>/akcesoria</url-pattern>
        <url-pattern>/wymienie</url-pattern>
        <url-pattern>/inne</url-pattern>
        <url-pattern>/audi</url-pattern>
        <url-pattern>/bmw</url-pattern>
        <url-pattern>/ford</url-pattern>
        <url-pattern>/renault</url-pattern>
        <url-pattern>/toyota</url-pattern>
        <url-pattern>/motocykle</url-pattern>
        <url-pattern>/czesci</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>adServlet</servlet-name>
        <url-pattern>/ogloszenie</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>userServlet</servlet-name>
        <url-pattern>/user</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
